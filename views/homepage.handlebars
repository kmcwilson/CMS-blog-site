
    <h1>Blog Forum</h1>
    {{#each allBlogs as |blog|}}
    <div class="card">
        <a href="/blog/{{id}}">
            <header id="user-name">{{user.name}}</header>
            <div class ="card-content">
                <p>{{blog.post}}</p>
            </div>
        </a>
    </div>
    {{/each}}
